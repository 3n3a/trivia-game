datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model GameSession {
    slug             String @id @default(uuid())
    questions        String
    opentdb_token    String
    score            Int    @default(0)
    current_question Int    @default(0)
    category_id      Int
    amount           Int
    joker_count      Int    @default(0)
}
